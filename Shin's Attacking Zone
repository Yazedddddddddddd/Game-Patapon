#include <stdio.h>
#include <stdlib.h>
#include <math.h>

#define MAX_N 1024

void convert_to_polar(int *coords, int N);

int main()
{
    int N, coords[MAX_N + 1][2];
    scanf("%d", &N);
    
    for (int i = 0; i < N; i++)
        scanf("%d%d", &coords[i][0], &coords[i][1]);
    
    
    convert_to_polar((int *) coords, N);
    
    return 0;
}
void convert_to_polar(int *coords, int N){
    for(int i=0,g=0;g<N;i+=2,g++){
        double r=0,angle=0;
        r=coords[i]*coords[i]+coords[i+1]*coords[i+1];
        r=sqrt(r);
        if(coords[i]!=0&&coords[i+1]!=0)
        angle=atan((float)(coords[i+1])/(float)(coords[i]))*180.0/M_PI;
        if(coords[i]>0&&coords[i+1]>0){
             ;
        }
        else if(coords[i]<0&&coords[i+1]>0){
             angle+=180.0;
        }
        else if(coords[i]<0&&coords[i+1]<0){
            angle+=180.0;
        }
        else if(coords[i]>0&&coords[i+1]<0){
            angle+=360.0;
        }
        else if(coords[i]==0&&coords[i+1]>0){
            angle=90.0;
        }
        else if(coords[i]==0&&coords[i+1]<0){
            angle=270.0;
        }
        else if(coords[i]<0&&coords[i+1]==0){
            angle=180.0;
        }
        else if(coords[i]>0&&coords[i+1]==0){
            angle=0.0;
        }
        else if(coords[i]==0&&coords[i+1]==0){
            r=0;
            angle=0;
        }
        printf("%.2f %.2f\n",angle,r);
    }
}
